<h5 style="margin-left: 2%;">Read</h5>

{{#each warehouses}}
<div class="container p-4">
    <div class="row">
        <div class="card" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">{{name}}</h5>
                <p class="card-text">{{headquarters_number}}</p>
                <p class="card-text">{{created_at}}</p>
                <p class="card-text">{{updated_at}}</p>
            </div>
        </div>
    </div>
</div>
{{/each}}